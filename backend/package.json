{
    "name": "Twistly",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "dependencies": {
        "agenda": "^0.9.1",
        "agenda-ui": "^0.0.7",
        "agendash": "^0.4.0",
        "async": "^1.5.2",
        "babel-cli": "^6.24.0",
        "babel-core": "^6.24.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-stage-2": "^6.22.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.15.0",
        "connect-mongo": "^0.8.2",
        "cookie-parser": "^1.4.1",
        "express": "^4.13.4",
        "express-jwt": "^5.1.0",
        "express-session": "^1.13.0",
        "jsonwebtoken": "^7.3.0",
        "loglevel": "^1.4.1",
        "method-override": "^2.3.3",
        "moment": "^2.12.0",
        "mongoose": "^4.4.6",
        "numeral": "^1.5.3",
        "passport": "^0.3.2",
        "passport-local": "^1.0.0",
        "passport-tumblr": "^0.1.2",
        "pug": "^2.0.0-rc.1",
        "snyk": "1.16.0",
        "tumblr.js": "0.0.7",
        "xo": "^0.18.2"
    },
    "devDependencies": {
        "ava": "^0.19.1",
        "babel-watch": "^2.0.6",
        "coveralls": "^2.13.0",
        "cross-env": "^4.0.0",
        "nyc": "^10.2.0",
        "supertest": "^3.0.0"
    },
    "private": true,
    "scripts": {
        "test": "xo && cross-env NODE_ENV=test 'nyc ava test/*.spec.js'",
        "start": "yarn start:production",
        "start:production": "cross-env NODE_ENV=production babel-node index.js",
        "start:development": "cross-env NODE_ENV=dev yarn start",
        "start:background": "cross-env NODE_ENV=production babel-node background/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/OmgImAlexis/wvvw.me"
    },
    "author": "OmgImAlexis",
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://github.com/OmgImAlexis/twistly/issues"
    },
    "homepage": "https://github.com/OmgImAlexis/twistly",
    "xo": {
        "space": 4,
        "rules": {
            "space-before-function-paren": [
                "error",
                "never"
            ]
        },
        "envs": [
            "node",
            "es6"
        ]
    },
    "ava": {
        "require": [
            "babel-core/register"
        ]
    },
    "nyc": {
        "exclude": [
            "test",
            "test{,-*}.js",
            "**/*.test.js",
            "**/__tests__/**",
            "**/node_modules/**"
        ]
    },
    "twistly": {
        "urls": {
            "production": "https://twistly.xyz",
            "development": "http://localhost:3000"
        },
        "port": {
            "production": 3000,
            "development": 3000
        }
    }
}
